<li id="manufacture_<%= manufacture.id %>" >
  <input class="item" type="checkbox">

  <%= manufacture.name %>, with gross
  <%= manufacture.gross %>
  <%= 'is active' if manufacture.active %>

  <%= link_to "Show", manufacture_path(manufacture), remote: true %>

  <%= link_to "Edit", edit_manufacture_path(manufacture) %>
  <%= link_to "Delete", manufacture_path(manufacture), method: :delete, remote: true %>
</li>

