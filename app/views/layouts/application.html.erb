<!DOCTYPE html>
<html>
<head>
  <title>Sysradio</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_user %>
    Hi <%= current_user.email%> <%= link_to "(get out!)", logout_path %>
  <% else %>
      <%= link_to 'Register', register_path %> or
      <%= link_to 'Enter', login_path %>
  <% end %>


  <hr />
  <%= flash_message %>



	<%= yield %>

</body>
</html>
